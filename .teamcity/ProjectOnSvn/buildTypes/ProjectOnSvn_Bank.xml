<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2018.1/project-config.xsd">
  <name>Bank</name>
  <description />
  <settings>
    <options>
      <option name="checkoutMode" value="ON_AGENT" />
      <option name="cleanBuild" value="true" />
    </options>
    <parameters>
      <param name="system.command.line.run.interpreter" value="false" />
      <param name="teamcity.svn.native.enabled" value="false" />
    </parameters>
    <build-runners>
      <runner id="RUNNER_1" name="" type="jb.nuget.installer">
        <parameters>
          <param name="nuget.path" value="%teamcity.tool.NuGet.CommandLine.DEFAULT%" />
          <param name="nuget.updatePackages.mode" value="sln" />
          <param name="nuget.use.restore" value="restore" />
          <param name="sln.path" value="Bank.sln" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
      <runner id="RUNNER_2" name="" type="VS.Solution">
        <parameters>
          <param name="build-file-path" value="Bank.sln" />
          <param name="msbuild_version" value="15.0" />
          <param name="run-platform" value="x86" />
          <param name="targets" value="Rebuild" />
          <param name="teamcity.step.mode" value="default" />
          <param name="toolsVersion" value="15.0" />
          <param name="vs.version" value="vs2017" />
        </parameters>
      </runner>
      <runner id="RUNNER_3" name="" type="VisualStudioTest">
        <parameters>
          <param name="dotNetCoverage.NCover.HTMLReport.File.Sort" value="0" />
          <param name="dotNetCoverage.NCover.HTMLReport.File.Type" value="1" />
          <param name="dotNetCoverage.NCover.Reg" value="selected" />
          <param name="dotNetCoverage.NCover.platformBitness" value="x86" />
          <param name="dotNetCoverage.NCover.platformVersion" value="v2.0" />
          <param name="dotNetCoverage.NCover3.Reg" value="selected" />
          <param name="dotNetCoverage.NCover3.args" value="//ias .*" />
          <param name="dotNetCoverage.NCover3.platformBitness" value="x86" />
          <param name="dotNetCoverage.NCover3.platformVersion" value="v2.0" />
          <param name="dotNetCoverage.NCover3.reporter.executable.args" value="//or FullCoverageReport:Html:{teamcity.report.path}" />
          <param name="dotNetCoverage.PartCover.Reg" value="selected" />
          <param name="dotNetCoverage.PartCover.includes" value="[*]*" />
          <param name="dotNetCoverage.PartCover.platformBitness" value="x86" />
          <param name="dotNetCoverage.PartCover.platformVersion" value="v2.0" />
          <param name="dotNetCoverage.dotCover.home.path" value="%teamcity.tool.JetBrains.dotCover.CommandLineTools.DEFAULT%" />
          <param name="dotNetCoverage.tool" value="dotcover" />
          <param name="dotNetTestRunner.Type" value="GenericProcess" />
          <param name="mstest_enable" value="checked" />
          <param name="teamcity.step.mode" value="default" />
          <param name="vstest_detect_team_city_logger" value="true" />
          <param name="vstest_engine" value="VSTest" />
          <param name="vstest_framework" value="default" />
          <param name="vstest_include" value="BankTest\bin\Debug\BankTest.dll" />
          <param name="vstest_platform" value="x86" />
          <param name="vstest_runner_path" value="%teamcity.dotnet.vstest.15.0%" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="ProjectOnSvn_SVNRepository">
        <checkout-rule rule="+:tags\tag_%env.rev_number% =&gt; ." />
      </vcs-entry-ref>
    </vcs-settings>
    <requirements />
    <build-triggers>
      <build-trigger id="vcsTrigger" type="vcsTrigger">
        <parameters>
          <param name="perCheckinTriggering" value="true" />
          <param name="quietPeriodMode" value="DO_NOT_USE" />
        </parameters>
      </build-trigger>
    </build-triggers>
    <cleanup />
  </settings>
</build-type>

