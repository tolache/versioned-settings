<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="902175a2-8416-4c85-9956-bf9addcfcba6" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2018.1/project-config.xsd">
  <name>Composite</name>
  <description />
  <settings>
    <options>
      <option name="buildConfigurationType" value="COMPOSITE" />
      <option name="showDependenciesChanges" value="true" />
    </options>
    <parameters>
      <param name="default.branch.name" value="%reverse.dep.BuildChain_BuildA.default.branch.name%" />
      <param name="reverse.dep.BuildChain_BuildA.default.branch.name" value="master" spec="text validationMode='not_empty' display='prompt'" />
    </parameters>
    <build-runners />
    <vcs-settings>
      <vcs-entry-ref root-id="BuildChain_RepoA">
        <checkout-rule rule="+:. =&gt; A" />
      </vcs-entry-ref>
      <vcs-entry-ref root-id="BuildChain_RepoB">
        <checkout-rule rule="+:. =&gt; B" />
      </vcs-entry-ref>
      <vcs-entry-ref root-id="BuildChain_RepoC">
        <checkout-rule rule="+:. =&gt; C" />
      </vcs-entry-ref>
    </vcs-settings>
    <requirements />
    <build-triggers />
    <dependencies>
      <depend-on sourceBuildTypeId="BuildChain_BuildB">
        <options>
          <option name="run-build-if-dependency-failed" value="RUN_ADD_PROBLEM" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
      <depend-on sourceBuildTypeId="BuildChain_BuildC">
        <options>
          <option name="run-build-if-dependency-failed" value="RUN_ADD_PROBLEM" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
    </dependencies>
    <cleanup />
  </settings>
</build-type>

