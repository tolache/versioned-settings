<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="6452ecb5-ef04-4fc4-a8f1-84175ca332f2" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2018.1/project-config.xsd">
  <name>Build 2</name>
  <description>additional build with no VCS root, made to test env. var. output</description>
  <settings>
    <disabled-settings>
      <setting-ref ref="ARTIFACT_DEPENDENCY_3" />
    </disabled-settings>
    <parameters>
      <param name="env.TESTVAR" value="testvalue" />
    </parameters>
    <build-runners>
      <runner id="RUNNER_13" name="" type="jetbrains_powershell">
        <parameters>
          <param name="jetbrains_powershell_edition" value="Core" />
          <param name="jetbrains_powershell_execution" value="PS1" />
          <param name="jetbrains_powershell_noprofile" value="true" />
          <param name="jetbrains_powershell_script_file" value="C:\Temp\script.ps1" />
          <param name="jetbrains_powershell_script_mode" value="FILE" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings />
    <requirements />
    <build-triggers />
    <artifact-dependencies>
      <dependency id="ARTIFACT_DEPENDENCY_3" sourceBuildTypeId="PowerShellTest_Build" cleanDestination="true">
        <revisionRule name="sameChainOrLastFinished" revision="latest.sameChainOrLastFinished" />
        <artifact sourcePath="+:TestFile.dat=&gt;." />
      </dependency>
    </artifact-dependencies>
    <cleanup />
  </settings>
</build-type>

